generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model pets {
  id           Int      @id @default(autoincrement())
  nome         String
  especie      String
  idade        Int
  dono         String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt

  @@map("pets")
}